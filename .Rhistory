results <- t.test(dose1.0,dose0.5, alternative = "two.sided", var.equal = T)
print(paste("results: p-value = ",
round(results$p.value,7) ,
", confidence interval: [",
round(results$conf[1],2),
", "
,round(results$conf[2],2), "]" ))
